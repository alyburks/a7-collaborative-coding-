#SINGLE FUNCTION
summary_info <- function(data) {
#MAKE LIST
ret <- list()
#ADD THINGS TO LIST
ret$total_data <-nrow(data) #81 responses to survey.
ret$total <- nrow(data %>%
filter(`Class Standing` == class) %>%
select(`Class Standing`))
######THIS IS A FUNCTION IN A FUNCTION :)
Class_standing <- function (data, class) {
total <- nrow(data %>%
filter(`Class Standing` == class) %>%
select(`Class Standing`))
return (total)
}
ret$total_fresh <- Class_standing(data, "Freshman") #15
ret$total_soph <- Class_standing(data, "Sophomore") #41
ret$total_jun <- Class_standing(data, "Junior") #18
ret$total_senior <- Class_standing(data, "Senior") #7
#RETURN LIST
return(ret)
}
